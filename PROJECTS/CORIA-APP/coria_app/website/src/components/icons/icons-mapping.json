  "transformationRules": {
    "importPattern": {
      "from": "import { IconName } from 'lucide-react'",
      "to": "import { Icon } from '@/components/icons/Icon'"
    },
    "usagePattern": {
      "from": "<IconName className=\"...\" size={24} />",
      "to": "<Icon name=\"icon-name\" className=\"...\" size={24} />"
    },
    "sizeMapping": {
      "16": 16,
      "18": 20,
      "20": 20,
      "24": 24,
      "28": 32,
      "32": 32
    },
    "accessibilityRules": {
      "decorative": {
        "pattern": "Icon inside button/link with text",
        "attributes": {
          "aria-hidden": "true"
        }
      },
      "meaningful": {
        "pattern": "Standalone icon without text",
        "attributes": {
          "title": "Descriptive text",
          "aria-label": "Descriptive text"
        }
      }
    }
  },
